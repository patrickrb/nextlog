# Database
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Database SSL Configuration
# Set to 'true' to enable SSL for database connections (recommended for production)
# Set to 'false' for local development
DATABASE_SSL=false

# Authentication
JWT_SECRET=your-secret-key-here

# Application
NEXTAUTH_URL=http://localhost:3000

# LoTW Integration
ENCRYPTION_SECRET=your-encryption-key-for-lotw-passwords

# Cron Jobs (for Vercel - optional for local development)
# This is used for additional security when cron jobs are triggered externally
# Vercel cron jobs run in a trusted environment, but this provides extra protection
CRON_SECRET=your-cron-secret-key